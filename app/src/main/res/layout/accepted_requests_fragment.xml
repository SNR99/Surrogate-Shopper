<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_width="match_parent"
				android:layout_marginTop="?attr/actionBarSize"
				android:padding="20dp"
				android:layout_height="match_parent">
	<TextView android:textColor="@color/darkGrey" android:paddingVertical="10dp" android:id="@+id/dsfsdf"
			  android:gravity="center" android:textSize="20dp"
			  android:text="My completed Requests"
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"/>
	<ListView android:layout_below="@+id/dsfsdf" android:id="@+id/accepted_list_view"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent"/>
	<TextView android:id="@+id/yrk" android:textSize="20sp" android:textColor="@color/darkGrey"
			  android:layout_centerInParent="true" android:text="Empty" android:layout_width="wrap_content"
			  android:layout_height="wrap_content"/>


</RelativeLayout>